{
  "name": "remix_project_lab",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve ./build/index.js",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve ./build/index.js",
    "test": "vitest --silent",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 BASE_URL=http://localhost:8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "test:dev": "cypress run --env url=dev",
    "test:dev:console": "npx cypress open --env url=dev",
    "test:3000": "cypress run --env url=local",
    "test:3000:console": "npx cypress open --env url=local",
    "test:prod": "cypress run --env url=prod",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "migrate-profiles-lake": "ts-node --project ./ts-node.tsconfig.json ./tasks/ProfilesMigration/migrateProfilesLake.ts",
    "get-gitprojects-activity": "ts-node --project ./ts-node.tsconfig.json ./tasks/GitHubDataManage/getGitHubActivityByProject.ts",
    "prepare": "husky install",
    "kysely-codegen": "kysely-codegen --out-file app/kysely.d.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@google-cloud/bigquery": "^6.0.3",
    "@material-table/core": "^6.3.2",
    "@material-table/exporters": "^1.2.19",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.8.3",
    "@mui/x-data-grid": "^5.17.26",
    "@prisma/client": "^5.4.2",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@remix-run/server-runtime": "*",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@uiw/react-markdown-preview": "^4.2.2",
    "@uiw/react-md-editor": "^3.20.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.0",
    "date-fns": "^2.29.2",
    "isbot": "^4",
    "kysely": "^0.27.2",
    "marked-react": "^1.2.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.5.1",
    "remix-auth-auth0": "^1.7.0",
    "remix-auth-oauth2": "^1.11.0",
    "remix-typedjson": "^0.4.1",
    "remix-validated-form": "^5.1.0",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@octokit/core": "^4.2.0",
    "@playwright/test": "^1.43.1",
    "@prisma/client": "^5.4.2",
    "@remix-run/dev": "*",
    "@remix-run/testing": "*",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.6.0",
    "@types/eslint": "^8.44.6",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "cypress": "^13.3.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.3",
    "kysely-codegen": "^0.11.0",
    "lint-staged": "^13.1.0",
    "msw": "^2.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prisma": "^5.4.2",
    "start-server-and-test": "^2.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.1",
    "vite-tsconfig-paths": "^4.3.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": "20"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
