{
  "name": "remix_project_lab",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "sst bind remix dev",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest --silent",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "test:dev": "cypress run --env url=dev",
    "test:dev:console": "npx cypress open --env url=dev",
    "test:3000": "cypress run --env url=local",
    "test:3000:console": "npx cypress open --env url=local",
    "test:prod": "cypress run --env url=prod",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "migrate-profiles-lake": "ts-node --project ./ts-node.tsconfig.json ./tasks/ProfilesMigration/migrateProfilesLake.ts",
    "get-gitprojects-activity": "ts-node --project ./ts-node.tsconfig.json ./tasks/GitHubDataManage/getGitHubActivityByProject.ts",
    "prepare": "husky install"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@google-cloud/bigquery": "^6.0.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.3",
    "@mui/x-data-grid": "^5.17.26",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/server-runtime": "^1.19.3",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@uiw/react-md-editor": "^3.20.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.0",
    "date-fns": "^2.29.2",
    "marked-react": "^1.2.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.5.1",
    "remix-auth-auth0": "^1.7.0",
    "remix-auth-oauth2": "^1.11.0",
    "remix-typedjson": "^0.1.6",
    "remix-validated-form": "^4.6.4",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@octokit/core": "^4.2.0",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/testing": "^1.19.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-v8": "^0.32.2",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "cypress": "^10.11.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prisma": "^4.3.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.32.2",
    "sst": "^2.40.1",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0"
  },
  "engines": {
    "node": "20"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
